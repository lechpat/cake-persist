{
    "name": "lechpat/cake-persist",
    "description": "CakePHP ORM plugin for persisting selected fields of related tables",
    "keywords": ["cakephp", "orm", "related", "materialize"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Saghy",
            "email": "segy@riesenia.com"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/orm": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Persist\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Persist\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
